{
  "name": "ts-glpi-client",
  "version": "1.0.0",
  "description": "A simple package to handle GLPI authentication and items",
  "main": "lib/main.js",
  "types": "lib/main.d.ts",
  "scripts": {
    "start": "ts-node src/main.ts",
    "clean": "rimraf lib",
    "tsc": "tsc",
    "build": "npm run clean && npm run tsc",
    "test": "jest",
    "prepublish": "npm run test && npm run build"
  },
  "keywords": [
    "GLPI",
    "TYPESCRIPT"
  ],
  "author": "Gilmar Antonio Ribeiro dos Santos",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/GilmardosSantos/TsGLPIClient"
  },
  "bugs": {
    "url": "https://github.com/GilmardosSantos/TsGLPIClient/issues"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "lib"
    ]
  },
  "files": [
    "lib/**/*"
  ],
  "homepage": "",
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsc": "^2.0.4",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@types/node": "^20.11.0",
    "axios": "^1.6.5",
    "dotenv": "^16.3.1"
  }
}
